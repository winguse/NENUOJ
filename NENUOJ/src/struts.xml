<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
    "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
	<package name="default" extends="struts-default,json-default">
		<interceptors>
			<interceptor name="userLoginNeeded"
				class="cn.edu.nenu.acm.oj.interceptors.UserLoginNeededInterceptor" />
		</interceptors>
		<global-results>
			<result name="reject" type="redirect">error.action?type=reject</result>
		</global-results>
	</package>
</struts>